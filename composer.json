{
    "name": "verbb/workflow",
    "description": "A Craft CMS plugin to create a workflow for publishing entries.",
    "type": "craft-plugin",
    "version": "1.5.7",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "workflow"
    ],
    "support": {
        "email": "support@verbb.io",
        "issues": "https://github.com/verbb/workflow/issues?state=open",
        "source": "https://github.com/verbb/workflow",
        "docs": "https://github.com/verbb/workflow",
        "rss": "https://github.com/verbb/workflow/commits/v2.atom"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Verbb",
            "homepage": "https://verbb.io"
        }
    ],
    "require": {
        "craftcms/cms": "^3.4.0",
        "verbb/base": "^1.0.2"
    },
    "autoload": {
        "psr-4": {
          "verbb\\workflow\\": "src/"
        }
    },
    "extra": {
        "name": "Workflow",
        "handle": "workflow",
        "changelogUrl": "https://raw.githubusercontent.com/verbb/workflow/craft-3/CHANGELOG.md",
        "class": "verbb\\workflow\\Workflow"
    }
}
